{
  "name": "vite-plugin-meta-env-type",
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "description": "generate vite ImportMetaEnv type",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuuui/vite-plugin-meta-env-type"
  },
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "changeset:release": "pnpm build && pnpm changeset:release:only"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "keywords": [
    "vite",
    "ImportMetaEnv",
    "env",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.3",
    "vite": "^7.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
